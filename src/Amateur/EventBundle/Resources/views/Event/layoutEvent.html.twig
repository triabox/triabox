	
	<div class="form-group">
		<label for="username">Nombre</label>
		<input name="{{ form.name.vars.full_name }}" value="{{ form.name.vars.value }}" type="username" class="form-control" id="username" placeholder="Enter Username" >
		{{ form_row(form._token) }}
	</div>
	
	<div class="form-group">
		<label for="exampleInputEmail1">{% trans %}Deporte{% endtrans %} </label>
		
		<div class="input-group" >
			<div class="form-group form-group-select2">
				{{ form_widget(form.sport) }}
			
			</div>
		</div>
	</div>

	<div class="form-group">
		<label for="exampleInputEmail1">{% trans %}Email{% endtrans %} </label>
		<input name="{{ form.emailContact.vars.full_name }}" value="{{ form.emailContact.vars.value }}"type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
	</div>
	
	
	<div class="form-group">
		
		 
		 <input id="{{ form.city.vars.id }}" class="controls" name="{{ form.city.vars.full_name }}" value="{{ form.city.vars.value }}"
			        placeholder="{% trans %}Ingrese Lugar{% endtrans %}">
			    <div id="type-selector" class="controls">
			      <input type="radio" name="type" id="changetype-all" checked="checked">
			      <label for="changetype-all">All</label>
			
			      <input type="radio" name="type" id="changetype-establishment">
			      <label for="changetype-establishment">Establishments</label>
			
			      <input type="radio" name="type" id="changetype-address">
			      <label for="changetype-address">Addresses</label>
			
			      <input type="radio" name="type" id="changetype-geocode">
			      <label for="changetype-geocode">Geocodes</label>
			    </div>
	</div>
	<div class="form-group">
						<label for="email">{% trans %}Donde es el evento?{% endtrans %}</label>
						<div class="main-box clearfix">
							
							<div class="main-box-body clearfix">
								<div id="map-geo" class="map-content" style="height: 400px; width: 750px"></div>
							</div>
						</div>
					
					
				</div>
	
	
