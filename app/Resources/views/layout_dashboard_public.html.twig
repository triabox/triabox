	<div id="fb-root"></div>
	<script>

	
	

	
	{% block script_facebook %}

	(function(d, s, id) {
		  var js, fjs = d.getElementsByTagName(s)[0];
		  if (d.getElementById(id)) return;
		  js = d.createElement(s); js.id = id;
		  js.src = "//connect.facebook.net/es_LA/sdk.js#xfbml=1&version=v2.5&appId=1600590050192428";
		  fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));
	
	{% endblock %}

	
	{% block script_twitter %}
	!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');
	
	{% endblock %}


	
	function moreData(div,link){

  	  	if(checkLogin()){  	
	 		$(div).load(link);
  	  	}

	}
	{% block ajax_public %}
	{% include 'EventBundle:Event:tapEventAjax.html.twig' %}

	
	{% endblock %}
	
	function loginFacebook(){
		$('#login').load('{{ url('hwi_oauth_service_redirect', { service: 'facebook' }) }}');
		}

	$.get( '{{ path('activity_logs_user') }}' );

	function checkLogin(){

		{% if app.user is  null %}
		
		$( "#login_button" ).click();
			
			return false;
		{% else %}
			return true;
		{% endif %}

			
		}
</script>
	
	
	
	{% endblock %}
	
